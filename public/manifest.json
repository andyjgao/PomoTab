{
  "manifest_version": 2,
  "name": "PomoTab",
  "author": "Andy J Gao",
  "version": "1.0.1",
  "description": "A Pomodoro Timer in each new tab.",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"]
}